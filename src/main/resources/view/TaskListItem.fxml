<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<fx:root fx:id="root" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="400.0" minHeight="60.0" prefHeight="63.0" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Rectangle fx:id="taskStatus" fill="#171717" height="56.0" stroke="#171717" strokeType="INSIDE" strokeWidth="0.0" width="8.0">
         <HBox.margin>
            <Insets left="10.0" />
         </HBox.margin></Rectangle>
        <ImageView fx:id="checkbox" fitHeight="16.0" fitWidth="16.0" pickOnBounds="true" preserveRatio="true">
         <HBox.margin>
            <Insets left="10.0" right="10.0" />
         </HBox.margin></ImageView>
      <VBox fx:id="taskDetails" alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="250.0" prefHeight="200.0" prefWidth="250.0">
         <children>
            <HBox maxWidth="250.0" prefHeight="16.0" prefWidth="250.0">
               <children>
                   <Label fx:id="taskNumber" maxWidth="250.0" minWidth="20.0" prefWidth="20.0" text="Label" textFill="#363636" wrapText="true">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin></Label>
                   <Label fx:id="taskDescription" maxWidth="220.0" minWidth="220.0" prefWidth="220.0" text="Label" textFill="#dedede" wrapText="true">
                     <font>
                        <Font name="SansSerif Regular" size="14.0" />
                     </font>
                  </Label>
                    <Label fx:id="badge" alignment="CENTER" maxHeight="13.0" maxWidth="56.0" minWidth="40.0" style="-fx-background-radius: 8;" text="Label" textFill="#363636" wrapText="true">
                     <font>
                        <Font name="SansSerif Regular" size="10.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="5.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <HBox fx:id="dateSection" maxHeight="1.7976931348623157E308" maxWidth="250.0" prefHeight="14.0" prefWidth="250.0">
               <children>
                    <ImageView fx:id="dateIcon" fitHeight="14.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                     <HBox.margin>
                        <Insets right="5.0" />
                     </HBox.margin>
                  </ImageView>
                    <Label fx:id="date" maxHeight="1.7976931348623157E308" maxWidth="200.0" minWidth="40.0" prefWidth="200.0" text="Label" textFill="#999999" wrapText="true">
                     <font>
                        <Font name="SansSerif Regular" size="10.0" />
                     </font>
                  </Label>
               </children>
               <VBox.margin>
                  <Insets bottom="5.0" top="5.0" />
               </VBox.margin>
            </HBox>
         </children>
         <HBox.margin>
            <Insets bottom="15.0" top="15.0" />
         </HBox.margin>
      </VBox>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
