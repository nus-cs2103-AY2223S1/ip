    <children>
      <TextFlow fx:id="textFlow" prefHeight="200.0" prefWidth="700.0" textAlignment="RIGHT" HBox.hgrow="ALWAYS">
         <children>
              <Label fx:id="dialog" text="Label" wrapText="true">
               <padding>
                  <Insets right="5.0" />
               </padding>
            </Label>
         </children>
         <HBox.margin>
            <Insets left="5.0" />
         </HBox.margin>
      </TextFlow>
        <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
