    <children>
        <Label fx:id="dialog" style="-fx-background-color: #85C1E9#85C1E9; -fx-background-radius: 10; -fx-label-padding: 6;" text="Label" wrapText="true">
            <HBox.margin>
                <Insets />
            </HBox.margin>
