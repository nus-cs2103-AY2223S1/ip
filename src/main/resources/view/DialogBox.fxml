<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<fx:root alignment="TOP_RIGHT" fillHeight="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <TextFlow style="-fx-background-color: #f5f3cb#f5f3cb; -fx-background-radius: 20;">
            <children>
                <Text fx:id="dialog" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
            </padding>
            <HBox.margin>
                <Insets left="10.0" right="10.0" />
            </HBox.margin>
        </TextFlow>
        <Circle fx:id="displayPicture" fill="#ffe41f" radius="40.0" stroke="TRANSPARENT" strokeType="INSIDE" />
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
    </padding>
</fx:root>
