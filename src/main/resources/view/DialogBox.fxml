<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<fx:root maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-border-color: AAAAAA;" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label fx:id="dialog" alignment="TOP_LEFT" minHeight="100.0" prefWidth="360.0" style="-fx-border-color: AAAAAA;" wrapText="true">
         <font>
            <Font name="Arial" size="16.0" />
         </font>
         <HBox.margin>
            <Insets />
         </HBox.margin>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding></Label>
        <ImageView fx:id="displayPicture" fitHeight="100.0" fitWidth="80.0">
         <HBox.margin>
            <Insets />
         </HBox.margin></ImageView>
    </children>
</fx:root>
