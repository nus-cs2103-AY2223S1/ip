<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308"
         maxWidth="1.7976931348623157E308"
         prefWidth="400.0" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/8.0.171">
    <children>
        <Label fx:id="dialog" minHeight="-Infinity" text="Label" wrapText="true"
               style="-fx-background-color: #ffffff; -fx-border-radius: 16; -fx-background-radius: 16; -fx-label-padding: 8; ">
            <HBox.margin>
                <Insets left="5.0" right="5.0"/>
            </HBox.margin>
            <font>
                <Font name="Courier New" size="12"/>
            </font>
        </Label>
        <ImageView fx:id="displayPicture" fitHeight="66.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true"/>
    </children>
    <padding>
        <Insets bottom="15.0" left="5.0" right="5.0" top="15.0"/>
    </padding>
</fx:root>
